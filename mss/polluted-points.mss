#polluted_points {
  marker-fill-opacity: 0.9;
  marker-line-color: #FFFFFF;
  marker-line-width: .2;
  marker-line-opacity: .7;
  marker-placement: point;
  marker-type: ellipse;
  marker-width: 7;
  marker-allow-overlap: false;
  [nag_id=10] {
    marker-fill: #9801d6;
    marker-width: 9;
  }
  [nag_id=7] {
    marker-fill: #f7b699;
    marker-width: 6;
  }
  [nag_id=8] {
    marker-fill: #a50008;
    marker-width: 9;
  }
  [nag_id=9] {
    marker-fill: #e171fb;
    marker-width: 6;
  }
}




