#industrialhistory{
  marker-file: url(http://com.cartodb.users-assets.production.s3.amazonaws.com/maki-icons/circle-18.svg);
  marker-fill-opacity: 0.9;
  marker-line-color: #FFF;
  marker-line-width: 1;
  marker-line-opacity: 1;
  marker-placement: point;
  marker-type: ellipse;
  marker-width: 25;
  marker-fill: #2E5387;
  marker-allow-overlap: true;
}

#industrialhistory{
  text-name: [id];
  text-face-name: 'DejaVu Sans Book';
  text-size: 10;
  text-label-position-tolerance: 10;
  text-fill: #fff;
  text-halo-fill: #FFF;
  text-halo-radius: 1;
  text-dy: 0;
  text-allow-overlap: true;
  text-placement: point;
  text-placement-type: simple;
}